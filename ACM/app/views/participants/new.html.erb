<h1>New participant</h1>

<% form_for(@participant) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :idpart %><br />
    <%= f.text_field :idpart %>
  </p>
  <p>
    <%= f.label :mat %><br />
    <%= f.text_field :mat %>
  </p>
  <p>
    <%= f.label :nom %><br />
    <%= f.text_field :nom %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', participants_path %>